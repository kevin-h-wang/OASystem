<configuration>
    <connectionStrings>
        <add name="DefaultConnection1" providerName="System.Data.SqlClient" connectionString="kevin-pc;Initial Catalog=Tuopu;user=sa;password=123456;" />
        <add name="DefaultConnection2" providerName="System.Data.SqlClient" connectionString="172.29.140.156\fis_cmd;Initial Catalog=DEV_CMD;user=sa;password=123qwe!@#;" />
